379     {\cf20 /*}\par
380 {\cf20         Template child class to create pools of the correct type.}\par
381 {\cf20     */}\par
382     {\cf17 template} <{\cf18 int} ITEM_SIZE>\par
383     {\cf17 class }MemPoolT : {\cf17 public} MemPool\par
384     \{\par
385     {\cf17 public}:\par
386         MemPoolT() : _blockPtrs(), _root(0), _currentAllocs(0), _nAllocs(0), _maxAllocs(0), _nUntracked(0) \{\}\par
387         ~MemPoolT()\par
388         \{\par
389             MemPoolT<ITEM_SIZE>::Clear();\par
390         \}\par
391 \par
392         {\cf18 void} Clear()\par
393         \{\par
394             {\cf20 // Delete the blocks.}\par
395             {\cf19 while} (!_blockPtrs.Empty())\par
396             \{\par
397                 Block *lastBlock = _blockPtrs.Pop();\par
398                 {\cf17 delete} lastBlock;\par
399             \}\par
400             _root = 0;\par
401             _currentAllocs = 0;\par
402             _nAllocs = 0;\par
403             _maxAllocs = 0;\par
404             _nUntracked = 0;\par
405         \}\par
406 \par
407         {\cf17 virtual} {\cf18 int} ItemSize(){\cf17  const}\par
408 {\cf17         }\{\par
409             {\cf19 return} ITEM_SIZE;\par
410         \}\par
411         {\cf18 int} CurrentAllocs(){\cf17  const}\par
412 {\cf17         }\{\par
413             {\cf19 return} _currentAllocs;\par
414         \}\par
415 \par
416         {\cf17 virtual} {\cf18 void} *Alloc()\par
417         \{\par
418             {\cf19 if} (!_root)\par
419             \{\par
420                 {\cf20 // Need a new block.}\par
421                 Block *block = {\cf17 new} Block;\par
422                 _blockPtrs.Push(block);\par
423 \par
424                 Item *blockItems = block->items;\par
425                 {\cf19 for} ({\cf18 int} i = 0; i < ITEMS_PER_BLOCK - 1; ++i)\par
426                 \{\par
427                     blockItems[i].next = &(blockItems[i + 1]);\par
428                 \}\par
429                 blockItems[ITEMS_PER_BLOCK - 1].next = 0;\par
430                 _root = blockItems;\par
431             \}\par
432             Item *{\cf17 const} result = _root;\par
433             TIXMLASSERT(result != 0);\par
434             _root = _root->next;\par
435 \par
436             ++_currentAllocs;\par
437             {\cf19 if} (_currentAllocs > _maxAllocs)\par
438             \{\par
439                 _maxAllocs = _currentAllocs;\par
440             \}\par
441             ++_nAllocs;\par
442             ++_nUntracked;\par
443             {\cf19 return} result;\par
444         \}\par
445 \par
446         {\cf17 virtual} {\cf18 void} Free({\cf18 void} *mem)\par
447         \{\par
448             {\cf19 if} (!mem)\par
449             \{\par
450                 {\cf19 return};\par
451             \}\par
452             --_currentAllocs;\par
453             Item *item = {\cf17 static_cast<}Item *{\cf17 >}(mem);\par
454 {\cf21 #ifdef TINYXML2_DEBUG}\par
455             memset(item, 0xfe, {\cf17 sizeof}(*item));\par
456 {\cf21 #endif}\par
457             item->next = _root;\par
458             _root = item;\par
459         \}\par
460         {\cf18 void} Trace({\cf17 const} {\cf18 char} *name)\par
461         \{\par
462             printf({\cf22 "Mempool %s watermark=%d [%dk] current=%d size=%d nAlloc=%d blocks=%d\\n"},\par
463                    name, _maxAllocs, _maxAllocs * ITEM_SIZE / 1024, _currentAllocs,\par
464                    ITEM_SIZE, _nAllocs, _blockPtrs.Size());\par
465         \}\par
466 \par
467         {\cf18 void} SetTracked()\par
468         \{\par
469             --_nUntracked;\par
470         \}\par
471 \par
472         {\cf18 int} Untracked(){\cf17  const}\par
473 {\cf17         }\{\par
474             {\cf19 return} _nUntracked;\par
475         \}\par
