220     {\cf20 /*}\par
221 {\cf20         A dynamic array of Plain Old Data. Doesn't support constructors, etc.}\par
222 {\cf20         Has a small initial memory pool, so that low or no usage will not}\par
223 {\cf20         cause a call to new/delete}\par
224 {\cf20     */}\par
225     {\cf17 template} <{\cf17 class} T, {\cf18 int} INITIAL_SIZE>\par
226     {\cf17 class }DynArray\par
227     \{\par
228     {\cf17 public}:\par
229         DynArray() : _mem(_pool),\par
230                      _allocated(INITIAL_SIZE),\par
231                      _size(0)\par
232         \{\par
233         \}\par
234 \par
235         ~DynArray()\par
236         \{\par
237             {\cf19 if} (_mem != _pool)\par
238             \{\par
239                 {\cf17 delete}[] _mem;\par
240             \}\par
241         \}\par
242 \par
243         {\cf18 void} Clear()\par
244         \{\par
245             _size = 0;\par
246         \}\par
247 \par
248         {\cf18 void} Push(T t)\par
249         \{\par
250             TIXMLASSERT(_size < INT_MAX);\par
251             EnsureCapacity(_size + 1);\par
252             _mem[_size] = t;\par
253             ++_size;\par
254         \}\par
255 \par
256         T *PushArr({\cf18 int} count)\par
257         \{\par
258             TIXMLASSERT(count >= 0);\par
259             TIXMLASSERT(_size <= INT_MAX - count);\par
260             EnsureCapacity(_size + count);\par
261             T *ret = &_mem[_size];\par
262             _size += count;\par
263             {\cf19 return} ret;\par
264         \}\par
265 \par
266         T Pop()\par
267         \{\par
268             TIXMLASSERT(_size > 0);\par
269             --_size;\par
270             {\cf19 return} _mem[_size];\par
271         \}\par
272 \par
273         {\cf18 void} PopArr({\cf18 int} count)\par
274         \{\par
275             TIXMLASSERT(_size >= count);\par
276             _size -= count;\par
277         \}\par
278 \par
279         {\cf18 bool} Empty(){\cf17  const}\par
280 {\cf17         }\{\par
281             {\cf19 return} _size == 0;\par
282         \}\par
283 \par
284         T &operator[]({\cf18 int} i)\par
285         \{\par
286             TIXMLASSERT(i >= 0 && i < _size);\par
287             {\cf19 return} _mem[i];\par
288         \}\par
289 \par
290         {\cf17 const} T &operator[]({\cf18 int} i){\cf17  const}\par
291 {\cf17         }\{\par
292             TIXMLASSERT(i >= 0 && i < _size);\par
293             {\cf19 return} _mem[i];\par
294         \}\par
295 \par
296         {\cf17 const} T &PeekTop(){\cf17  const}\par
297 {\cf17         }\{\par
298             TIXMLASSERT(_size > 0);\par
299             {\cf19 return} _mem[_size - 1];\par
300         \}\par
301 \par
302         {\cf18 int} Size(){\cf17  const}\par
303 {\cf17         }\{\par
304             TIXMLASSERT(_size >= 0);\par
305             {\cf19 return} _size;\par
306         \}\par
307 \par
308         {\cf18 int} Capacity(){\cf17  const}\par
309 {\cf17         }\{\par
310             TIXMLASSERT(_allocated >= INITIAL_SIZE);\par
311             {\cf19 return} _allocated;\par
312         \}\par
313 \par
314         {\cf18 void} SwapRemove({\cf18 int} i)\par
315         \{\par
316             TIXMLASSERT(i >= 0 && i < _size);\par
317             TIXMLASSERT(_size > 0);\par
318             _mem[i] = _mem[_size - 1];\par
319             --_size;\par
320         \}\par
321 \par
322         {\cf17 const} T *Mem(){\cf17  const}\par
323 {\cf17         }\{\par
324             TIXMLASSERT(_mem);\par
325             {\cf19 return} _mem;\par
326         \}\par
327 \par
328         T *Mem()\par
329         \{\par
330             TIXMLASSERT(_mem);\par
331             {\cf19 return} _mem;\par
332         \}\par
333 \par
334     {\cf17 private}:\par
335         DynArray({\cf17 const} DynArray &);       {\cf20 // not supported}\par
336         {\cf18 void} operator=({\cf17 const} DynArray &); {\cf20 // not supported}\par
