368     \{\par
369     {\cf17 public}:\par
370         MemPool() \{\}\par
371         {\cf17 virtual} ~MemPool() \{\}\par
372 \par
373         {\cf17 virtual} {\cf18 int} ItemSize() {\cf17 const} = 0;\par
374         {\cf17 virtual} {\cf18 void} *Alloc() = 0;\par
