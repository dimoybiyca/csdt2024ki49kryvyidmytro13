338         {\cf18 void} EnsureCapacity({\cf18 int} cap)\par
339         \{\par
340             TIXMLASSERT(cap > 0);\par
341             {\cf19 if} (cap > _allocated)\par
342             \{\par
343                 TIXMLASSERT(cap <= INT_MAX / 2);\par
344                 {\cf17 const} {\cf18 int} newAllocated = cap * 2;\par
345                 T *newMem = {\cf17 new} T[newAllocated];\par
346                 TIXMLASSERT(newAllocated >= _size);\par
347                 memcpy(newMem, _mem, {\cf17 sizeof}(T) * _size); {\cf20 // warning: not using constructors, only works for PODs}\par
348                 {\cf19 if} (_mem != _pool)\par
349                 \{\par
350                     {\cf17 delete}[] _mem;\par
351                 \}\par
352                 _mem = newMem;\par
353                 _allocated = newAllocated;\par
354             \}\par
355         \}\par
356 \par
357         T *_mem;\par
358         T _pool[INITIAL_SIZE];\par
359         {\cf18 int} _allocated; {\cf20 // objects allocated}\par
360         {\cf18 int} _size;      {\cf20 // number objects in use}\par
361     \};\par
